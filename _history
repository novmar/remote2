o\")\nradio.setGroup(153)\nlet X0 = pins.analogReadPin(AnalogReadWritePin.P1)\nlet Y0 = pins.analogReadPin(AnalogReadWritePin.P2)\nfor (let index = 5; index >= 0; index--) {\n    basic.showNumber(index)\n    pins.digitalWritePin(DigitalPin.P12, 1)\n    basic.pause(50)\n    pins.digitalWritePin(DigitalPin.P12, 0)\n    basic.pause(50)\n\n}\nbasic.showIcon(IconNames.Rollerskate)\nbasic.forever(function () {\n    // read values\n    X = pins.analogReadPin(AnalogReadWritePin.P1)\n    Y = pins.analogReadPin(AnalogReadWritePin.P2)\n    serial.writeLine(\"X=\" + X + \"; Y=\" + Y)\n    forward = (X - 512) * 100 / 512 // to percentage\n    turn = (Y - 512) * 100 / 512    // to percentage\n    // dead zone\n    if (Math.abs(forward) < forwardgap) forward = 0\n    if (Math.abs(turn) < turngap ) turn = 0\n    // tank mix\n    lmotor = forward + turn   // levý motor\n    rmotor = forward - turn   // pravý motor\n\n    // limit\n    rmotor = Math.clamp(-100, 100, rmotor)\n    lmotor = Math.clamp(-100, 100, lmotor)\n    \n    // ODESLÁNÍ DO ROBOTA\n    if (lmotor != rmotor)  { radio.sendValue(\"mr\", rmotor); rmotor.old = rmotor }\n    radio.sendValue(\"ml\", lmotor)\n\n})\n","README.md":"","pxt.json":"{\n    \"name\": \"remote2\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}}],"shares":[],"lastSaveTime":1766878735704}